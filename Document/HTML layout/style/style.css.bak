/**Begin Login page**/

.log_in{
	  width:600px;
	  float:left;
	  border-right:1px solid #222;
	  margin:5px auto;
	  padding: 15px 20px;
	  
  }
  .log-form {
  width: 100%;
  float:left;
  min-width: 320px;
  max-width: 510px;
  background: #fff;

  }
  
  
   .log_in form {
    display: block;
    width: 100%;
    padding: 2em;
  }
  
  .log_in h2 {
    width: 100%;
    color: lighten(@grey, 20%);
    font-size: 25px;
    display: block;

    width: 100%;
    text-transform: uppercase;
    padding: .75em 1em .75em 1.5em;
    box-shadow:inset 0px 1px 1px fadeout(white, 95%);
    border: 1px solid darken(@grey, 5%);
  
    margin: 0;
    font-weight: 200;
  }
  
  .log_in input {
    display: block;
    margin: auto auto;
    width: 100%;
    margin-bottom: 2px;
    padding: 10px 0;
    border: none;
    border-bottom: 1px solid #eaeaea;
  }
  
   .log_in .btn {
    display: inline-block;
    border: 1px solid darken(@blue, 5%);
    padding: 10px 40px;
	margin: 10px 55px;

    }
  .log_in input:focus{
    outline: none;
}
  
  .log_in .forgot {
    color: lighten(@blue, 10%);
    line-height: 10px;
    text-decoration: none;
    font-size: 12px;
	position:relative;
	top: 35px;
    margin:0;
    padding: 0;
    float: right;
  
    }
	.right_side{
		width:400px;
		float:left;
		margin:5px auto;
	  padding: 15px 20px;
		
	}

.right_side h1.right_logo{
	display:block;
	line-height:50px;
	float:left;
	color: #323;
	text-align:center;
	font-weight:bold;
	font-size:40px;
	padding: 86px 0 0 30px;
}
/** END LOGIN **/

